  },
  "tokenColors": [
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.logical.python",
        "storage.type"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "operator",
        "keyword.operator",
        "keyword.operator.comparison.python"
      ],
      "settings": {
        "foreground": "#489EB7"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "String Punctuation",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#6D8F51"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#D08770"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#DCA694"
      }
    },
    {
      "name": "Builtin Constant",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#C05E3F"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#6C7A96"
      }
    },
    // {
    //   "name": "Comment Punctuation",
    //   "scope": "punctuation.definition.comment",
    //   "settings": {
    //     "foreground": "#646A76"
    //   }
    // },
    {
      "name": "Class",
      "scope": [
        // "meta.class"
        // "entity.name.class"
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Enum",
      "scope": [
        // "storage.type.enum",
        "entity.name.enum"
        // "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#F2DEB6"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function"
        // "entity.name.function.member"
        // "support.function",
        // "meta.function"
      ],
      "settings": {
        // "foreground": "#5E81AC",
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Builtin Function",
      "scope": ["support.function.builtin"],
      "settings": {
        "foreground": "#5E81AC"
      }
    },
    {
      "name": "Magic Function",
      "scope": ["support.function.magic"],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Function Decorator",
      "scope": [
        // "entity.name.function.decorator"
        "meta.function.decorator.python",
        "punctuation.definition.decorator.python"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "name": "Bold",
      "scope": ["storage.type"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": ["comment", "keyword.control", "entity.other.inherited-class"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
